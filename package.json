  "license": "(MIT OR Apache-2.0)",
  "devDependencies": {
    "@types/sinon": "^17.0.3",
    "hono": "^4.4.6",
    "prettier": "^3.3.2",
    "sinon": "^18.0.0",
    "tsx": "^4.15.4",
    "typescript": "^5.4.5",
    "uvu": "^0.5.6",
    "watchlist": "^0.3.1"
  },
  "dependencies": {
    "nanoevents": "^9.0.0"
  }
}
