{
  "name": "sse-feed",
  "version": "0.0.1",
  "description": "SSE as a ReadableStream",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "./node_modules/.bin/tsx ./node_modules/uvu/bin.js . .+\\.test\\.ts",
    "test:watch": "./node_modules/.bin/watchlist --eager src -- pnpm run test"
